{
  "name": "npm-snapshot",
  "version": "1.1.0",
  "description": "Tiny tool for releasing npm prerelease snapshots.",
  "author": "Oscar Dominguez <dominguez.celada@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oscard0m/npm-snapshot.git"
  },
  "bugs": {
    "url": "https://github.com/oscard0m/npm-snapshot/issues"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 2.12.0"
  },
  "main": "index.js",
  "bin": {
    "npm-snapshot": "./bin/npm-snapshot"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  }
}
